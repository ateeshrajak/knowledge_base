# pyinstaller
# Remember to update both setuptools and pyinstaller 
# pip install --upgrade setuptools
# pip install --upgrade pyinstaller

# Convert python script to executable
pyinstaller script.py --onefile

# Convert python module to executable
pyinstaller --paths pymodule/ --onefile pymodule/__main__.py -n exename

# Convert python module to executable from GNU/Linux for Windows (wine)
wine ~/.wine/drive_c/Python38/Scripts/pyinstaller.exe --onefile 18176.py

